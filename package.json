{
  "name": "demopenguin",
  "version": "1.0",
  "description": "A demo Chrome Extension with a React popup styled with Tailwind CSS using TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "watch": "webpack --config webpack.config.js --watch",
